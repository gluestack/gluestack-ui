    "DateTimePickerTrigger": "@/components/ui/datetimepicker",
    "DateTimePickerInput": "@/components/ui/datetimepicker",
    "DateTimePickerIcon": "@/components/ui/datetimepicker",
    "DateTimePickerPortal": "@/components/ui/datetimepicker",
    "DateTimePickerBackdrop": "@/components/ui/datetimepicker",
    "DateTimePickerContent": "@/components/ui/datetimepicker",
    "DateTimePickerCalendar": "@/components/ui/datetimepicker",
    "DateTimePickerCalendarHeader": "@/components/ui/datetimepicker",
    "DateTimePickerCalendarGrid": "@/components/ui/datetimepicker",
    "DateTimePickerCalendarDay": "@/components/ui/datetimepicker",
    "DateTimePickerActionBar": "@/components/ui/datetimepicker",
    "DateTimePickerActionButton": "@/components/ui/datetimepicker",
    "CalendarDaysIcon": "@/components/ui/icon",
    "useState": "react"
  }
}
