{
  "title": "Basic",
  "argTypes": {
    "mode": {
      "control": {
        "type": "select"
      },
      "options": ["date", "time", "datetime"],
      "defaultValue": "date"
    },
    "variant": {
      "control": {
        "type": "select"
      },
      "options": ["underlined", "outline", "rounded"],
      "defaultValue": "outline"
    },
    "size": {
      "control": {
        "type": "select"
      },
      "options": ["sm", "md", "lg", "xl"],
      "defaultValue": "md"
    }
  },
  "reactLive": {
