{
  "name": "kitchen-sink",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --reset-cache",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "DARK_MODE=media expo start --android",
    "ios": "DARK_MODE=media expo start --ios",
    "web": "DARK_MODE=media expo start --web",
    "lint": "expo lint",
    "build": "cd ../.. && yarn install && yarn sync:kitchen-sink && cd apps/kitchen-sink && yarn add @gluestack-ui/core @gluestack-ui/utils && expo export --platform web --output-dir dist",
    "build:preview": "cd ../.. && yarn install && yarn sync:kitchen-sink && cd apps/kitchen-sink && yarn add @gluestack-ui/core@preview @gluestack-ui/utils@preview && expo export --platform web --output-dir dist"
  },
  "dependencies": {
    "@expo/html-elements": "^0.10.1",
    "@expo/vector-icons": "^14.1.0",
    "@legendapp/motion": "^2.4.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "dom-helpers": "^6.0.1",
    "expo": "~53.0.13",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.1.1",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-updates": "~0.28.15",
    "expo-web-browser": "~14.2.0",
    "lucide-react-native": "^0.523.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-aria": "^3.41.1",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "react-stately": "^3.39.0",
    "tailwind-variants": "^0.1.20",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}
