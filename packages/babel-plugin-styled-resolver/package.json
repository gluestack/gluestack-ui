{
  "name": "@gluestack-style/babel-plugin-styled-resolver",
  "version": "0.1.14-alpha.1",
  "description": "A gluestack-style babel plugin that transpiles your styled function calls and resolves the component styling in build time.",
  "keywords": [
    "css-in-js",
    "babel-plugin",
    "server-side rendering",
    "ssr"
  ],
  "main": "lib/index",
  "module": "lib/index",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "prepare": "yarn build",
    "build": "babel src/index.js -d lib",
    "dev": "babel src/index.js -d lib",
    "clean": "rm -rf lib",
    "dev:web": "cd example/native && yarn web --clear"
  },
  "peerDependencies": {
    "@gluestack-style/react": "^0.1.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/preset-env": "^7.20.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/react": "^18.0.22",
    "@types/react-native": "^0.69.15",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.70.3",
    "react-native-builder-bob": "^0.20.1",
    "react-native-web": "^0.18.1",
    "tsconfig": "*",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/generator": "^7.20.5",
    "@babel/parser": "^7.20.5",
    "@babel/plugin-transform-typescript": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/traverse": "^7.20.5",
    "lodash.merge": "^4.6.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      [
        "module",
        {
          "babelrc": true
        }
      ]
    ]
  },
  "files": [
    "lib/",
    "src/"
  ]
}
