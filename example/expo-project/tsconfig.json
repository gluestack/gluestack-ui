{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "ui-components": ["../storybook/src/ui-components/index"],
      "@gluestack-ui/actionsheet": ["../../packages/actionsheet/src"],
      "@gluestack-ui/avatar": ["../../packages/avatar/src"],
      "@gluestack-ui/pressable": ["../../packages/pressable/src"],
      "@gluestack-ui/button": ["../../packages/button/src"],
      "@gluestack-ui/center": ["../../packages/center/src"],
      "@gluestack-ui/checkbox": ["../../packages/checkbox/src"],
      "@gluestack-ui/divider": ["../../packages/divider/src"],
      "@gluestack-ui/heading": ["../../packages/heading/src"],
      "@gluestack-ui/hstack": ["../../packages/hstack/src"],
      "@gluestack-ui/icon": ["../../packages/icon/src"],
      "@gluestack/ui": ["../../packages/button/src"],
      "@gluestack-ui/tooltip": ["../../packages/tooltip/src"],
      "@gluestack-ui/popover": ["../../packages/popover/src"],
      "@gluestack-ui/provider": ["../../packages/provider/src"],
      "@gluestack-ui/textarea": ["../../packages/textarea/src"],
      "@gluestack-ui/input": ["../../packages/input/src"],
      "@gluestack-ui/switch": ["../../packages/switch/src"],
      "@gluestack-ui/radio": ["../../packages/radio/src"],
      "@gluestack-ui/spinner": ["../../packages/spinner/src"],
      "@gluestack-ui/slider": ["../../packages/slider/src"],
      "@gluestack-ui/link": ["../../packages/link/src"],
      "@gluestack-ui/progress": ["../../packages/progress/src"],
      "@gluestack-ui/menu": ["../../packages/menu/src"],
      "@gluestack-ui/select": ["../../packages/select/src"],
      "@gluestack-ui/modal": ["../../packages/modal/src"],
      "@gluestack-ui/fab": ["../../packages/fab/src"],
      "@gluestack-ui/tabs": ["../../packages/tabs/src"],
      "@gluestack-ui/toast": ["../../packages/toast/src"],
      "@gluestack-ui/alert-dialog": ["../../packages/alert-dialog/src"],
      "@gluestack-ui/alert": ["../../packages/alert/src"],
      "@gluestack-ui/form-control": ["../../packages/form-control/src"],
      "@gluestack-ui/overlay": ["../../packages/overlay/src"],
      "@gluestack-ui/stack": ["../../packages/stack/src"],
      "@gluestack-ui/vstack": ["../../packages/vstack/src"],
      "@gluestack-style/react": [
        "../../packages/dank-style/packages/react/src"
      ],
      "@gluestack-ui/transitions": ["../../packages/transitions/src"]
    },
    "emitDeclarationOnly": true,
    "noEmit": false,
    "declaration": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": true,
    "esModuleInterop": true,
    "importsNotUsedAsValues": "error",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  },
  "exclude": ["lib", "node_modules", "storybookDocsComponents"],
  "include": ["src"]
}
